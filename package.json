  "bugs": {
    "url": "https://github.com/yanfengliu/turkey/issues"
  },
  "homepage": "https://github.com/yanfengliu/turkey#readme",
  "dependencies": {
    "@types/cypress-image-snapshot": "^3.1.6",
    "cypress": "^8.7.0",
    "cypress-image-snapshot": "^4.0.1",
    "prettier": "^2.3.2",
    "serve": "^14.1.2",
    "start-server-and-test": "^1.13.1",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "husky": "^7.0.0",
    "pretty-quick": "^3.1.1"
  }
